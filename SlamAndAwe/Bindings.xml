<Bindings>
	<Binding name="SLAMANDAWE_WATER_SHIELD" header="SLAMANDAWE_TITLE">
		SlamAndAwe:CastWaterShield();
	</Binding>
	<Binding name="SLAMANDAWE_LIGHTNING_SHIELD">
		SlamAndAwe:CastLightningShield();
	</Binding>
	<Binding name="SLAMANDAWE_MHWEAPON_REBUFF">
		SlamAndAwe:RebuffWeapons(16);
	</Binding>
	<Binding name="SLAMANDAWE_OHWEAPON_REBUFF">
		SlamAndAwe:RebuffWeapons(17);
	</Binding>
	<Binding name="SLAMANDAWE_ENABLE_DISABLE">
		SlamAndAwe:EnableDisable();
	</Binding>
	<Binding name="SLAMANDAWE_PRIORITYSET_1">
		SlamAndAwe:SelectPrioritySet(1);
	</Binding>
	<Binding name="SLAMANDAWE_PRIORITYSET_2">
		SlamAndAwe:SelectPrioritySet(2);
	</Binding>
	<Binding name="SLAMANDAWE_PRIORITYSET_3">
		SlamAndAwe:SelectPrioritySet(3);
	</Binding>
	<Binding name="SLAMANDAWE_PRIORITYSET_4">
		SlamAndAwe:SelectPrioritySet(4);
	</Binding>
	<Binding name="SLAMANDAWE_PRIORITYSET_5">
		SlamAndAwe:SelectPrioritySet(5);
	</Binding>
</Bindings>
