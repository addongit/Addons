			</Frame>
		</Frames>
		<Scripts>
			<OnHide>
				AAHVar.BrowseItemLink = nil
				AAHVar.PriceHistoryProcessing = true
			</OnHide>
			<OnMouseWheel>
				UIPanelScrollBar_OnMouseWheel(AAH_HistoryListScrollBar, delta)
			</OnMouseWheel>
		</Scripts>
