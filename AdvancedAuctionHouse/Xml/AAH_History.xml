<Ui xmlns="http://www.runewaker.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.runewaker.com/UI.xsd">
	<Frame name="AAH_HistoryListItemTemplate" virtual="true" hidden="true">
		<Size>
			<AbsDimension x="520" y="36"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\Common\PanelSeparate-HorizontalLine">
					<Size>
						<AbsDimension x="578" y="4"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="-4"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString name="$parentName" inherits="GameFontHighlight" justifyH="LEFT" text="item name">
					<Size>
						<AbsDimension x="98" y="32"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT">
							<Offset>
								<AbsDimension x="12" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentDateText" inherits="GameFontHighlight" justifyH="LEFT" text="05/10 09:18">
					<Size>
						<AbsDimension x="88" y="32"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentName" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentSellerText" inherits="GameFontHighlight" justifyH="LEFT" text="seller_name">
					<Size>
						<AbsDimension x="88" y="32"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentDateText" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentBuyerText" inherits="GameFontHighlight" justifyH="LEFT" text="buyer_name">
					<Size>
						<AbsDimension x="80" y="32"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentSellerText" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentPPU" inherits="GameFontHighlight" justifyH="RIGHT" text="1234567">
					<Size>
						<AbsDimension x="80" y="32"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentBuyerText" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parentIcon" inherits="ItemButtonTemplate">
				<Anchors>
					<Anchor point="LEFT">
						<Offset>
							<AbsDimension x="-30" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture name="$parentRed" file="Interface\Buttons\Debuff_Border" alphaMode="ADD">
							<Size>
								<AbsDimension x="36" y="36"/>
							</Size>
							<Anchors>
								<Anchor point="CENTER">
								</Anchor>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnEnter>
						if this.index > 10 then
							GameTooltip:Hide() <!--need a dummy tooltip to explain why no tooltip-->
						else
							GameTooltip:SetOwner(this, "ANCHOR_TOPRIGHT")
							GameTooltip:SetHistoryItem(GetAuctionHistoryItemNums() + 1 - this.index)
						end
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide()
					</OnLeave>
				</Scripts>
			</Button>
			<Frame name="$parentPrice" inherits="MoneyFrameTemplate">
				<Anchors>
					<Anchor point="RIGHT">
						<Offset>
							<AbsDimension x="24" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						MoneyFrameTemplate_SetMode(this, "copper")
						MoneyFrameTemplate_SetType(this, "STATIC")
					</OnLoad>
				</Scripts>
			</Frame>
		</Frames>
		<Scripts>
			<OnMouseWheel>
				UIPanelScrollBar_OnMouseWheel(AAH_HistoryListScrollBar, delta)
			</OnMouseWheel>
		</Scripts>
	</Frame>
